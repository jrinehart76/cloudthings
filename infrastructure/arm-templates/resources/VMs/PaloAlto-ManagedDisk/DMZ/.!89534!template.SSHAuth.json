{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.13",
  "parameters": {
    "virtualMachineName": {
      "type": "string"
    },
    "vmSize": {
      "type": "string",
      "allowedValues": [
        "Standard_D3",
        "Standard_D4",
        "Standard_D3_v2",
        "Standard_D4_v2",
        "Standard_D5_v2",
        "Standard_D3_v2_Promo",
        "Standard_D4_v2_Promo",
        "Standard_D5_v2_Promo",
        "Standard_DS3_v2",
        "Standard_DS4_v2",
        "Standard_DS5_v2",
        "Standard_DS3_v2_Promo",
        "Standard_DS4_v2_Promo",
        "Standard_DS5_v2_Promo",
        "Standard_A4"
      ],
      "defaultValue": "Standard_DS3_v2_Promo"
    },
    "imageVersion": {
      "type": "string",
      "allowedValues": [
        "latest",
        "8.0.0",
        "7.1.1"
      ],
      "metadata": {
        "description": "Version number of PAN-OS VM-Series"
      },
      "defaultValue": "8.0.0"
    },
    "osDiskStorageType": {
      "type": "string",
      "metadata": {
        "description": "Standard or Premium storage for OS disk"
      },
      "allowedValues": [
        "Standard_LRS",
        "Premium_LRS"
      ],
      "defaultValue": "Premium_LRS"
    },
    "publisher": {
      "type": "string",
      "defaultValue": "paloaltonetworks",
      "metadata": {
        "description": "The VM image reference Publisher"
      }
    },
    "offer": {
      "type": "string",
      "defaultValue": "vmseries1",
      "metadata": {
        "description": "The VM image reference Offer"
      }
    },
    "sku": {
      "type": "string",
      "defaultValue": "byol",
      "metadata": {
        "description": "The VM image reference SKU"
      }
    },
    "version": {
      "type": "string",
      "defaultValue": "latest",
      "metadata": {
        "description": "The version of the VM image reference offer"
      }
    },
    "adminUsername": {
      "type": "string"
    },
    "sshKey": {
      "type": "string",
      "metadata": {
        "description": "SSH rsa public key file as a string"
      }
    },
    "virtualNetworkRGName": {
      "type": "string"
    },
    "virtualNetworkName": {
      "type": "string"
    },
    "mgmtSubnetName": {
      "type": "string",
      "defaultValue": "Mgmt",
      "metadata": {
        "description": "Subnet for Management NIC"
      }
    },
    "mgmtIPAddress": {
      "type": "string",
      "metadata": {
        "description": "Static IP Address for Management NIC"
      }
    },
    "untrustSubnetName": {
      "type": "string",
      "defaultValue": "Untrust",
      "metadata": {
        "description": "Subnet for Untrust NIC"
      }
    },
    "untrustIPAddress": {
      "type": "string",
      "metadata": {
        "description": "Static IP Address for Untrust NIC"
      }
    },
    "trustSubnetName": {
      "type": "string",
      "defaultValue": "Trust",
      "metadata": {
        "description": "Subnet for Trust NIC"
      }
    },
    "trustIPAddress": {
      "type": "string",
      "metadata": {
        "description": "Static IP Address for Trust NIC"
      }
    },
    "availabilitySetName": {
      "type": "string"
    },
    "availabilitySetUpdateDomainCount": {
      "type": "int"
    },
    "availabilitySetFaultDomainCount": {
      "type": "int"
    },
    "environment": {
      "type": "string",
      "defaultValue": "Dev",
      "metadata": {
        "description": "Environment Name - Dev, Uat, Prod, POC"
      },
      "minLength": 1,
      "maxLength": 4
    },
    "application": {
      "type": "string",
      "defaultValue": "LMX",
      "metadata": {
        "description": "Application - LMX, ELX, etc"
      }
    },
    "region": {
      "type": "string",
      "defaultValue": "AM",
      "metadata": {
        "description": "Geographical Region - AM, EU, AP"
      }
    },
    "applicationName": {
      "type": "string",
      "defaultValue": "NA",
      "metadata": {
        "description": "ResourceTag - Example Name: FSS_BPC_Integration"
      }
    },
    "applicationOwner": {
      "type": "string",
      "defaultValue": "NA",
      "metadata": {
        "description": "ResourceTag - Example Owner: Sai Pathak"
      }
    },
    "applicationDeveloper": {
      "type": "string",
      "defaultValue": "NA",
      "metadata": {
        "description": "ResourceTag - FSS_BPC_Integration"
      }
    },
    "applicationID": {
      "type": "string",
      "defaultValue": "NA",
      "metadata": {
        "description": "ResourceTag - "
      }
    },
    "applicationDescription": {
      "type": "string",
      "defaultValue": "NA",
      "metadata": {
