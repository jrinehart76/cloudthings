{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Kubernetes",
      "orchestratorVersion": "#{KubernetesVersion}#",
      "kubernetesConfig": {
        "apiServerConfig": {
          "--admission-control": "NamespaceLifecycle,LimitRanger,ServiceAccount,DefaultStorageClass,ResourceQuota,AlwaysPullImages"
        },
        "kubeletConfig": {
          "--max-pods": "30"
        },
        "privateCluster": {
          "enabled": true
        },
        "cloudProviderBackoff": true,
        "cloudProviderBackoffDuration": 5,
        "cloudProviderBackoffExponent": 1.5,
        "cloudProviderBackoffJitter": 1,
        "cloudProviderBackoffRetries": 6,
        "cloudProviderRateLimit": true,
        "cloudProviderRatelimitBucket": 10,
        "cloudProviderRateLimitQPS": 3,
        "networkPolicy": "azure",
        "enableRbac": true,
        "addons": [
          {
            "name": "kubernetes-dashboard",
            "containers": [
              {
                "name": "kubernetes-dashboard",
                "cpuRequests": "50m",
                "memoryRequests": "512Mi",
                "cpuLimits": "50m",
                "memoryLimits": "512Mi"
              }
            ]
          }
        ]
      }
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "acs-engine",
      "vmSize": "Standard_D2_v3",
      "vnetSubnetId": "/subscriptions/****-*****-*****/resourceGroups/rgName/providers/Microsoft.Network/virtualNetworks/vnetName/subnets/subnetName",
      "firstConsecutiveStaticIP": "1.1.1.250",
      "vnetCidr": "#{ACSVNetCIDR}#",
      "ipAddressCount": #{ACSIPCount}#
    },
    "agentPoolProfiles": [
      {
        "name": "agents",
        "count": 3,
        "vmSize": "Standard_D2_v3",
        "availabilityProfile": "AvailabilitySet",
        "vnetSubnetId": "/subscriptions/****-*****-*****/resourceGroups/rgName/providers/Microsoft.Network/virtualNetworks/vnetName/subnets/subnetName",
        "storageProfile": "ManagedDisks",
        "customNodeLabels": {
          "services": "linux"
        }
      }
    ],
    "linuxProfile": {
      "adminUsername": "cloudadmin",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "ssh-rsa xxxxxxxxxxxxxxxxxxxx"
          }
        ]
      }
    },
    "servicePrincipalProfile": {
      "clientId": "servicePrincipalId",
      "secret": "servicePrincipalKey"
    },
    "aadProfile": {
      "serverAppID": "#{AADServerAppId}#",
      "clientAppID": "#{AADClientAppId}#",
      "tenantID": "00000000-0000-0000-0000-tenant000001"
    }
  }
}